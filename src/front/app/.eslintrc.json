{
  "globals" : {
    "React" : true,
    "componentHandler" : true,
    "describe": true,
    "it": true,
    "expect": true,
    "jest" : true
  },
  "extends": "airbnb",
  "rules" :{
      "no-multi-spaces"  : 0,
      "key-spacing" : [1,{
        "beforeColon" : true,
        "afterColon"  : true,
        "mode"        : "minimum"
      }],
      "no-use-before-define" : 0,
      "no-param-reassign" : 0,
      "comma-dangle" : 0,
      "react/react-in-jsx-scope" : 0
    }
}
