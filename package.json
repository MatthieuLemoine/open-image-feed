{
  "name": "open-image-feed",
  "version": "1.0.0",
  "description": "A open source self-hosted real-time image feed.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index",
    "build": "./node_modules/webpack/bin/webpack.js --config webpack/browser/webpack.config.browser.js --progress --colors",
    "watch": "./node_modules/webpack/bin/webpack.js --config webpack/browser/webpack.config.browser.js --progress --colors --watch",
    "prod": "./node_modules/webpack/bin/webpack.js --config webpack/browser/webpack.config.browser.prod.js --progress --colors",
    "test": "./node_modules/jest-cli/bin/jest.js",
    "lint": "./node_modules/eslint/bin/eslint.js ./src --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "check": "npm test && npm run lint && npm run prod",
    "build-android": "./node_modules/webpack/bin/webpack.js --config webpack/mobile/webpack.config.android.js --progress --colors",
    "build-ios": "./node_modules/webpack/bin/webpack.js --config webpack/mobile/webpack.config.ios.js --progress --colors",
    "watch-android": "./node_modules/webpack/bin/webpack.js --config webpack/mobile/webpack.config.android.js --progress --colors --watch",
    "watch-ios": "./node_modules/webpack/bin/webpack.js --config webpack/mobile/webpack.config.ios.js --progress --colors --watch",
    "mobile-start": "node ./node_modules/react-native/local-cli/cli.js start --root ./src/mobile/",
    "android": "node ./node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node ./node_modules/react-native/local-cli/cli.js run-ios",
    "ios-bundle": "node ./node_modules/react-native/local-cli/cli.js bundle --dev false --entry-file src/mobile/index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "node ./node_modules/react-native/local-cli/cli.js bundle --entry-file src/mobile/index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android-bundle": "node ./node_modules/react-native/local-cli/cli.js bundle --dev false --entry-file src/mobile/index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "node ./node_modules/react-native/local-cli/cli.js bundle --entry-file src/mobile/index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle"
  },
  "jest": {
    "bail": false,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest",
    "setupEnvScriptFile": "./test/setup-env.js",
    "setupTestFrameworkScriptFile": "node_modules/jasmine-enzyme/lib/jest.js",
    "testPathDirs": [
      "./src"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "redux",
      "enzyme",
      "jasmine-enzyme",
      "deep-freeze"
    ],
    "verbose": false
  },
  "dependencies": {
    "basic-auth": "^1.0.4",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "multer": "^1.1.0",
    "node-uuid": "^1.4.7",
    "path": "^0.12.7",
    "socket.io": "^1.4.8",
    "thinky": "^2.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-jest": "^14.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "bootstrap": "^3.3.6",
    "compression-webpack-plugin": "^0.3.1",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "image-webpack-loader": "^1.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-enzyme": "^1.1.0",
    "jest-cli": "^14.0.0",
    "jsx-loader": "^0.13.2",
    "lodash": "^4.13.1",
    "material-design-lite": "^1.1.3",
    "moment": "^2.14.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-dropzone": "^3.5.3",
    "react-native": "^0.31.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatthieuLemoine/open-image-feed.git"
  },
  "keywords": [
    "feed",
    "open",
    "image"
  ],
  "author": "MatthieuLemoine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MatthieuLemoine/open-image-feed/issues"
  },
  "homepage": "https://github.com/MatthieuLemoine/open-image-feed#readme"
}
